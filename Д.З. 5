import random
print("Это игра - угадай число.")
print("Тебе нужно угадать число от 1 до 100")
l = input("Как тебя зовут?")
o = 0
u = random.randint(1, 100)
while o < 10:
    a = int(input(print(l, "Введи число")))
    o += 1
    print(l, "ты ввёл число", a)
    if u > a:
        print("Моё число больше")
    elif u < a:
        print("Моё число меньше")
    elif u == a:
        print(l, "ты угадал моё число!")
